{
  "name": "typescript-postmessagehandler",
  "version": "4.0.0",
  "description": "A TypeScript PostMessage handler",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --progress --config ./webpack.config.js",
    "check": "tsc --project tsconfig.json --noEmit",
    "docs": "typedoc",
    "fmt": "prettier -c src",
    "lint": "eslint -c .eslintrc.js src",
    "postbuild": "npm run fmt && npm run lint",
    "test": "echo \"Error: no tests implemented\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/friedemannsommer/typescript-postmessagehandler.git"
  },
  "keywords": [
    "typescript",
    "postmessage",
    "handler"
  ],
  "files": [
    "dist/*"
  ],
  "author": "Friedemann Sommer <contact@friedemannsommer.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/friedemannsommer/typescript-postmessagehandler/issues"
  },
  "homepage": "https://github.com/friedemannsommer/typescript-postmessagehandler#readme",
  "devDependencies": {
    "@mxssfd/typedoc-theme": "1.1.2",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.0",
    "eslint": "8.43.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "prettier": "2.8.8",
    "ts-loader": "9.4.3",
    "typedoc": "0.24.8",
    "typedoc-plugin-mdn-links": "3.0.3",
    "typescript": "5.1.3",
    "webpack": "5.88.0",
    "webpack-cli": "5.1.4"
  }

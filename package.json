{
  "name": "typescript-postmessagehandler",
  "version": "3.0.0",
  "description": "A TypeScript PostMessage handler",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx webpack --progress --config ./webpack.config.js",
    "check": "npx tsc --project tsconfig.json --noEmit",
    "fmt": "npx prettier -c src",
    "lint": "npx eslint -c .eslintrc.js src",
    "postbuild": "npm run fmt && npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/friedemannsommer/typescript-postmessagehandler.git"
  },
  "keywords": [
    "typescript",
    "postmessage",
    "handler"
  ],
  "files": [
    "dist/*"
  ],
  "author": "Friedemann Sommer <contact@friedemannsommer.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/friedemannsommer/typescript-postmessagehandler/issues"
  },
  "homepage": "https://github.com/friedemannsommer/typescript-postmessagehandler#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "eslint": "8.20.0",
    "eslint-webpack-plugin": "3.2.0",
    "prettier": "2.7.1",
    "ts-loader": "9.3.1",
    "typescript": "4.7.4",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  }
}

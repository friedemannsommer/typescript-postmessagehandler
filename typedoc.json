{
  "$schema": "https://typedoc.org/schema.json",
  "cleanOutputDir": false,
  "entryPoints": [
    "src/index.ts"
  ],
  "githubPages": true,
  "htmlLang": "en",
  "includeVersion": true,
  "name": "TypeScript PostMessage",
  "out": "docs",
  "plugin": [
    "@mxssfd/typedoc-theme",
    "typedoc-plugin-mdn-links",
    "typedoc-plugin-versions"
  ],
  "theme": "my-theme"
}